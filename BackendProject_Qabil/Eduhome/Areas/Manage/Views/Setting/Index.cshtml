@model Setting
@{
    ViewData["Title"] = "Index";
}

<div id="page-wrapper">
    <div id="page-inner">
        <div class="row d-flex j-c-sb a-i-c before_after-none">
            <div class="col-md-3">
                <h1>Setting</h1>
            </div>
            <div class="col-md-3">
                <a asp-action="update" class="waves-effect waves-light btn btn-warning" style="width:100%">Update</a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="card teal">
                    <div class="card-content white-text">
                        <span class="card-title">Title Logo</span>
                        <div class="image">
                            <img style="width: auto; max-height: 42px" src="/uploads/setting/@Model.TitleLogo" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card teal">
                    <div class="card-content white-text">
                        <span class="card-title">Header Logo</span>
                        <div class="image">
                            <img style="width: 100%; max-height: 42px" src="/uploads/setting/@Model.HeaderLogo" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card teal">
                    <div class="card-content white-text">
                        <span class="card-title">Colorly Logo</span>
                        <div class="image">
                            <img style="width: 100%; max-height: 42px" src="/uploads/setting/@Model.ColorLogo" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card teal">
                    <div class="card-content white-text">
                        <span class="card-title">Footer Logo</span>
                        <div class="image">
                            <img style="width: 100%; max-height: 42px" src="/uploads/setting/@Model.FooterLogo" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="card teal">
                    <div class="card-content white-text">
                        <span class="card-title">About Image</span>
                        <div class="image">
                            <img style="width: 100%" src="/uploads/setting/@Model.AboutPic" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 col-sm-6">
                <div class="row ml-0 m-0">
                    <div class="col-12">
                        <div class="card teal">
                            <div class="card-content white-text">
                                <span class="card-title">Why Choose Title</span>
                                <p>
                                    @Model.ChooseTitle
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="card teal">
                            <div class="card-content white-text">
                                <span class="card-title">Why Choose Text</span>
                                <p>
                                    @Model.ChooseText
                                </p>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="col-md-6 col-sm-6">
                <div class="row m-0">
                    <div class="col-12">
                        <div class="card blue-grey darken-1">
                            <div class="card-content white-text">
                                <span class="card-title">About Title</span>
                                <p>
                                    @Model.AboutTitle
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="card blue-grey darken-1">
                            <div class="card-content white-text">
                                <span class="card-title">About Title</span>
                                <p>
                                    @Model.AboutDesc
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
           
        </div>
        <div class="row">
            <div class="col-md-6 col-sm-6">
                <div class="row m-0">
                    <div class="col-md-6">
                        <div class="card orange">
                            <div class="card-content white-text">
                                <span class="card-title">Phone 1</span>
                                <p>
                                    @Model.Phone1
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="card orange">
                            <div class="card-content white-text">
                                <span class="card-title">Phone 2</span>
                                <p>
                                    @Model.Phone2
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="card orange">
                            <div class="card-content white-text">
                                <span class="card-title">E-mail</span>
                                <p>
                                    @Model.Email
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="card orange">
                            <div class="card-content white-text">
                                <span class="card-title">Site</span>
                                <p>
                                    @Model.Site
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-sm-6">
                <div class="row m-0">
                    <div class="col-12">
                        <div class="card green">
                            <div class="card-content">
                                <span class="card-title">Address</span>
                                <p>
                                    @Model.Address
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="card green">
                            <div class="card-content">
                                <span class="card-title">About Video</span>
                                <p>
                                    @Model.AboutVideo
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="row m-0">
                    <div class="col-md-3">
                        <div class="card facebook">
                            <div class="card-content white-text">
                                <span class="card-title">Facebook</span>
                                <p>
                                    @Model.Facebook
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="card pinterest">
                            <div class="card-content white-text">
                                <span class="card-title">Pinterest</span>
                                <p>
                                    @Model.Pinteres
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="card twitter">
                            <div class="card-content white-text">
                                <span class="card-title">Twitter</span>
                                <p>
                                    @Model.Twitter
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="card vimeo">
                            <div class="card-content white-text">
                                <span class="card-title">Vimeo</span>
                                <p>
                                    @Model.Vimeo
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

